{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "Auroraphtgrp",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "lint-staged",
    "module:g": "nest g module modules/$npm_config_name",
    "controller:g": "nest g controller modules/$npm_config_name",
    "service:g": "nest g service modules/$npm_config_name"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.2.0",
    "@prisma/client": "^5.15.1",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "argon2": "^0.31.1",
    "axios": "^1.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "lodash": "^4.17.21",
    "logform": "^2.4.2",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.3.7",
    "nestjs-prisma": "^0.23.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "prisma-extension-soft-delete": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^15.1.0",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "test:watch": "jest --detectOpenHandles --watch",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1"
    }
  }
}
